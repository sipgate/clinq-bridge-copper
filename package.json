{
  "name": "clinq-bridge-copper",
  "version": "1.0.0",
  "description": "CLINQ Bridge for Copper",
  "main": "dist/index.js",
  "repository": "git@github.com:sipgate/clinq-bridge-copper.git",
  "contributors": [
